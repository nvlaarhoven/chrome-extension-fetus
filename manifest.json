{
  "version": "0.0.4",
  "name": "Pregnancy - Fetus Size",
  "description": "How big is my baby this week?",
  "permissions": [
    "storage",
    "https://www.nhs.uk/"
  ],
  "browser_action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "src/images/icon16.png",
      "48": "src/images/icon48.png",
      "128": "src/images/icon128.png"
    }
  },
  "icons": {
    "16": "src/images/icon16.png",
    "48": "src/images/icon48.png",
    "128": "src/images/icon128.png"
  },
  "manifest_version": 2
}
